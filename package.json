  "scripts": {
    "dev": "vite",
    "typecheck": "tsc -b --pretty false",
    "build": "npm run typecheck && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ci": "CI=1 eslint . --max-warnings=0",
    "lint:coverage": "eslint . && npm run validate:coverage",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage --reporter=default --reporter=json --outputFile=coverage/vitest-report.json",
    "test:coverage:ci": "vitest run --coverage --reporter=json --outputFile=coverage/vitest-report.json",
    "validate:coverage": "npm run test:coverage && node scripts/validate-coverage.js",
    "test:ui": "vitest --ui",
    "verify:tooling": "tsx scripts/verify-tooling.ts",
    "validate:tooling": "npm run lint && npm run build && npm run test:coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@sonar/scan": "^4.3.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
