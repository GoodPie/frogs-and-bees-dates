{
  "name": "free",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "dependencies": {
    "@chakra-ui/react": "^3.26.0",
    "@emotion/react": "^11.14.0",
    "dayjs": "^1.11.7",
    "firebase": "^9.15.0",
    "next-themes": "^0.4.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-icons": "^5.5.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:coverage": "eslint . --ext .js,.jsx,.ts,.tsx && npm run test:coverage:validate",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:coverage:validate": "vitest --coverage --run --reporter=verbose",
    "test:coverage:ci": "vitest --coverage --run --reporter=json --outputFile=coverage/coverage-summary.json",
    "validate:coverage": "node scripts/validate-coverage.js",
    "test:ui": "vitest --ui"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^12.0.0",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "3.2.4",
    "eslint-plugin-testing-library": "^7.8.0",
    "jsdom": "^27.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  }
}
