    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "9.38.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jsdom": "^27.0.1",
    "resize-observer-polyfill": "^1.5.1",
    "tsx": "^4.20.6",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.1",
    "vite": "npm:rolldown-vite@7.1.14",
    "vitest": "3.2.4"
  }
