{
  "name": "free",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "dependencies": {
    "@chakra-ui/react": "^3.26.0",
    "@emotion/react": "^11.14.0",
    "@firebase/rules-unit-testing": "^5.0.0",
    "@testing-library/user-event": "^14.6.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.7",
    "firebase": "^12.3.0",
    "firebase-admin": "^13.5.0",
    "firebase-functions": "^6.6.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.4",
    "web-vitals": "^5.0.0",
    "zod": "^4.1.12"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint src/ vite.config.ts --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ vite.config.ts --ext .js,.jsx,.ts,.tsx --fix",
    "lint:coverage": "eslint . --ext .js,.jsx,.ts,.tsx && npm run test:coverage:validate",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:coverage:validate": "vitest --coverage --run --reporter=verbose",
    "test:coverage:ci": "vitest --coverage --run --reporter=json --outputFile=coverage/coverage-summary.json",
    "validate:coverage": "node scripts/validate-coverage.js",
    "test:ui": "vitest --ui"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@sonar/scan": "^4.3.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.38.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jsdom": "^27.0.1",
    "resize-observer-polyfill": "^1.5.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "npm:rolldown-vite@7.1.14",
    "vitest": "^3.2.4"
  }
}
